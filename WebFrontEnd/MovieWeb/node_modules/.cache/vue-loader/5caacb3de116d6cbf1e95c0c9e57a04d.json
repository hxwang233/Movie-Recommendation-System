te.userInfo) {\r\n\t\t\t\t\tlet {\r\n\t\t\t\t\t\tdata\r\n\t\t\t\t\t} = await this.$get(this.$store.state.requestPath + 'allMovies');\r\n\t\t\t\t\tthis.dataSource = data.data;\r\n\t\t\t\t\tthis.dataloading = false;\r\n\t\t\t\t} else {\r\n\t\t\t\t\tconsole.log(\"userInfo not found\");\r\n\t\t\t\t}\r\n\t\t\t},\r\n\t\t\tasync search(){\r\n\t\t\t\tthis.dataloading = true;\r\n\t\t\t\tlet {\r\n\t\t\t\t\tdata\r\n\t\t\t\t} = await this.axios.post(this.requestPath + 'admin_select', this.params);\r\n\t\t\t\tthis.dataSource = data.data;\r\n\t\t\t\tthis.dataloading = false;\r\n\t\t\t},\r\n\t\t\tonChange(date, dateString) {\r\n\t\t\t\tthis.params.startTime = dateString[0];\r\n\t\t\t\tthis.params.endTime = dateString[1];\r\n\t\t\t},\r\n\t\t\tasync confirm(e) {\r\n\t\t\t\tconsole.log('del_mid:' + e);\t\r\n\t\t\t\tlet {\r\n\t\t\t\t\tdata\r\n\t\t\t\t} = await this.$get(this.$store.state.requestPath + 'delMovie/' + e);\r\n\t\t\t\tif (data.code == 0){\r\n\t\t\t\t\tthis.$message.success('删除成功');\r\n\t\t\t\t\tconst dataSource = [...this.dataSource];\r\n\t\t\t\t\tthis.dataSource = dataSource.filter(item => item.mid !== e);\r\n\t\t\t\t}else{\r\n\t\t\t\t\tthis.$message.error('删除失败');\r\n\t\t\t\t}\t\r\n\t\t\t},\r\n\t\t\tcancel(e) {\r\n\t\t\t\tconsole.log(e);\r\n\t\t\t},\r\n\t\t\tshowModal() {\r\n\t\t\t\tthis.visible = true;\r\n\t\t\t},\r\n\t\t\tupdatePopWinState(state){\r\n\t\t\t\tthis.visible = state;\r\n\t\t\t},\r\n\t\t\tasync updateDataSource(newData){\r\n\t\t\t\tconsole.log(newData);\r\n\t\t\t\tconst { dataSource } = this;\r\n\t\t\t\tthis.dataSource = [...dataSource, newData];\r\n\t\t\t\tthis.getAllMovie();\r\n\t\t\t},\r\n\t\t},\r\n\t};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n</style>\n"]}]}