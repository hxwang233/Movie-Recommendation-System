t\tif (data.code == 0){\r\n\t\t\t\t\tthis.$message.success('删除成功');\r\n\t\t\t\t\tconst dataSource = [...this.dataSource];\r\n\t\t\t\t\tthis.dataSource = dataSource.filter(item => item.mid !== e);\r\n\t\t\t\t}else{\r\n\t\t\t\t\tthis.$message.error('删除失败');\r\n\t\t\t\t}\t\r\n\t\t\t},\r\n\t\t\tcancel(e) {\r\n\t\t\t\tconsole.log(e);\r\n\t\t\t},\r\n\t\t\tshowModal() {\r\n\t\t\t\tthis.visible = true;\r\n\t\t\t},\r\n\t\t\tupdatePopWinState(state){\r\n\t\t\t\tthis.visible = state;\r\n\t\t\t},\r\n\t\t\tasync updateDataSource(newData){\r\n\t\t\t\tconsole.log(newData);\r\n\t\t\t\tconst { dataSource } = this;\r\n\t\t\t\tthis.dataSource = [...dataSource, newData];\r\n\t\t\t\tthis.getAllMovie();\r\n\t\t\t},\r\n\t\t},\r\n\t};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n</style>\n"],"sourceRoot":"src/components/admin"}]}